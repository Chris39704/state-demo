  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "build:dev:dll": "cross-env FORCE_DLL=true node_modules/webpack/bin/webpack.js --mode development --config ./config/library.webpack.config.js",
    "clean:dll": "rm -rf dll",
    "clean:nodemodules": "rm -rf node_modules",
    "z-postinstall": "npm run build:dev:dll",
    "lint:all": "eslint -c ./.eslintrc.json './**/*'",
    "lint:all:ext": "eslint -c ./.eslintrc.json './**/*.{js,jsx,ts,tsx}'",
    "lint:all:fix": "eslint -c ./.eslintrc.json './**/*.{js,jsx,ts,tsx}' --fix",
    "lint:src": "eslint -c ./.eslintrc.json './src/**/*.{js,jsx,ts,tsx}'",
    "lint:src:fix": "eslint -c ./.eslintrc.json './src/**/*.{js,jsx,ts,tsx}' --fix",
    "lint:src:fixi": "eslint --fix --ignore-path ./.eslintignore -c ./.eslintrc.json src ",
    "prestart": "npm run build:dev:dll",
    "test": "node scripts/test.js",
    "watch": "tsc -b -w",
    "ram-link-node-modules": "mkdir /Volumes/ramdisk/node_modules && rm -r -f ./node_modules && ln -s /Volumes/ramdisk/node_modules ./node_modules"
  },
  "engines": {
    "yarn": "\n\nPlease use NPM....\n\n"
  },
  "quokka": {
    "babel": true,
    "stdEsm": true,
    "plugins": [
      "jsdom-quokka-plugin"
    ]
  },
  "babel": {
    "plugins": [
      "react-hot-loader/babel"
    ],
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [
      "/Users/chrisfarabaugh/Documents/workspace/redux-demo/src"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  }
}